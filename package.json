{
  "name": "react-app-boilerplate",
  "version": "1.0.0",
  "scripts": {
    "clean": "rm -r dist",
    "dev": "yarn run build:dev && yarn start",
    "prod": "yarn run build:prod && yarn start",
    "build:dev": "yarn run build:client:dev && yarn run build:server:dev",
    "build:prod": "yarn run build:client:prod && yarn run build:server:prod",
    "start": "node build/server.js",
    "build:client:dev": "webpack --config=webpack/webpack.client.dev.js",
    "build:client:prod": "webpack --config=webpack/webpack.client.prod.js",
    "build:server:dev": "webpack --mode development --config=webpack/webpack.server.js",
    "build:server:prod": "webpack --mode production --config=webpack/webpack.server.js",
    "analytics": "webpack --config=webpack/webpack.analytics.js",
    "test": "jest --watchAll",
    "coverage": "jest --coverage",
    "updateSnapshot": "jest --updateSnapshot"
  },
  "repository": "git@github.com:slashtu/react-app-boilerplate.git",
  "author": "Slash Tu <y12341234@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.18.0",
    "body-parser": "~1.18.2",
    "cookie-parser": "~1.4.3",
    "debug": "~4.1.1",
    "express": "^4.16.4",
    "http-proxy": "^1.17.0",
    "morgan": "~1.9.0",
    "react": "^16.8.5",
    "react-dom": "^16.8.5",
    "react-redux": "^7.0.0-beta.0",
    "react-router": "^5.0.0",
    "react-router-dom": "^5.0.0",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "serve-favicon": "~2.5.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.0",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-proposal-decorators": "^7.4.0",
    "@babel/polyfill": "^7.4.0",
    "@babel/preset-env": "^7.4.2",
    "@babel/preset-react": "^7.0.0",
    "autoprefixer": "^9.5.1",
    "babel-jest": "^24.7.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-module-resolver": "^3.2.0",
    "css-loader": "^2.1.1",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.11.2",
    "enzyme-to-json": "^3.3.5",
    "identity-obj-proxy": "^3.0.0",
    "incstr": "^1.2.3",
    "jest": "^24.7.1",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.11.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss": "^7.0.14",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "precss": "^4.0.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "terser-webpack-plugin": "^1.2.3",
    "webpack": "^4.29.6",
    "webpack-bundle-analyzer": "^3.1.0",
    "webpack-cli": "^3.3.0",
    "webpack-dev-middleware": "^3.6.1",
    "webpack-merge": "^4.2.1",
    "webpack-node-externals": "^1.7.2",
    "workbox-webpack-plugin": "^4.2.0"
  },
  "engines": {
    "node": "^10.13.0"
  }
}
